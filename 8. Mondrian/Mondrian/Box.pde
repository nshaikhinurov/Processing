public class Box{
  public color boxColor;
  public float bx1;
  public float bx2;
  public float by1;
  public float by2;

  public Box(float bx1, float by1, float bx2, float by2, color boxColor){
    this.bx1 = bx1;
    this.bx2 = bx2;
    this.by1 = by1;
    this.by2 = by2;
    this.boxColor = boxColor;
  }
}
